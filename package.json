{
  "name": "abb-starter",
  "version": "1.0.0",
  "description": "",
  "main": "src/app.tsx",
  "scripts": {
    "pack": "webpack",
    "serve": "webpack-dev-server --open --hot --inline",
    "lint": "tslint 'src/**/*.ts?'",
    "build": "set NODE_ENV=production & npm run lint & npm run test & npm run pack",
    "test": "set NODE_ENV=test & mocha --recursive --compilers ts:ts-node/register --require ignore-styles --bail -- src/**/*.spec.tsx",
    "cover": "set NODE_ENV=test && nyc npm t",
    "start": "npm run serve"
  },
  "author": "ABB",
  "license": "MIT",
  "nyc": {
    "include": [
      "**/*.spec.tsx?"
    ],
    "exclude": [
      "typings"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "json",
      "html"
    ],
    "all": true
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "@types/chai": "^3.4.35",
    "@types/enzyme": "^2.7.5",
    "@types/es6-promise": "0.0.32",
    "@types/mocha": "^2.2.39",
    "@types/react": "^15.0.13",
    "@types/react-dom": "^0.14.23",
    "@types/react-redux": "^4.4.36",
    "@types/react-router": "^2.0.49",
    "@types/redux": "^3.6.0",
    "@types/sinon": "^1.16.35",
    "awesome-typescript-loader": "^3.0.8",
    "chai": "^3.5.0",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.1",
    "ignore-styles": "^5.0.1",
    "jsdom": "9.11.0",
    "jsdom-global": "2.1.1",
    "mocha": "^3.2.0",
    "nyc": "^10.1.2",
    "react-addons-test-utils": "^15.4.2",
    "sinon": "^1.17.7",
    "source-map-loader": "^0.1.6",
    "style-loader": "^0.13.1",
    "ts-node": "^2.1.0",
    "tslint": "^4.5.0",
    "tslint-react": "^2.4.0",
    "typescript": "^2.2.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  }
}
